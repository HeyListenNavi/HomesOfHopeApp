{
    "$schema": "https://json.schemastore.org/tsconfig.json",
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "~/*": ["*"],
        }
    },
    "include": ["**/*.ts", "**/*.tsx", "nativewind-env.d.ts"]
}
